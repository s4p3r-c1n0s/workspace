import java.util.*;

class GUIStudent {
	private Student student;
	
	
	public void getAll(){	
		Scanner sc = new Scanner(System.in);
		System.out.println("Name : ");
		student.setName(new String(sc.nextLine()));
		System.out.println("Marks : ");
		student.setMarks((int)sc.nextInt());
		sc.nextLine();
		System.out.println("Grade : ");
		student.setGrade((char)sc.nextLine().charAt(0));
	}
}
